  "fabio:hasPart": [
    {
      "@id": "section-1",
      "@type": "fabio:Section",
      "dcterms:title": "Introduction",
      "fabio:hasContent": "Knee osteoarthritis (KOA) is characterized by cartilage impairment and inflammation. This study examines AD-MSCs as a noninvasive therapy."
    },
    {
      "@id": "section-2",
      "@type": "fabio:Section",
      "dcterms:title": "Methods",
      "fabio:hasContent": "40 patients were randomized to receive either AD-MSCs or placebo, with follow-ups using MRI and biomarker analysis."
    },
    {
      "@id": "section-3",
      "@type": "fabio:Section",
      "dcterms:title": "Results",
      "fabio:hasContent": "Significant improvement in WOMAC scores observed in the AD-MSCs group, along with positive MRI changes in cartilage thickness."
    },
    {
      "@id": "section-4",
      "@type": "fabio:Section",
      "dcterms:title": "Discussion",
      "fabio:hasContent": "The results highlight the potential of AD-MSCs in treating KOA and emphasize the importance of inflammation modulation."
    }
  ],
