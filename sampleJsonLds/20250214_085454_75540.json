  "fabio:hasPart": [
    {
      "@id": "section1",
      "@type": "fabio:Section",
      "dcterms:title": "Introduction",
      "fabio:hasContent": "Acute myeloid leukemia (AML) is a hematopoietic malignancy involving clonal hematopoiesis and defects in differentiation, proliferation, and cell death, resulting in the accumulation of immature blasts."
    },
    {
      "@id": "section2",
      "@type": "fabio:Section",
      "dcterms:title": "Methods",
      "fabio:hasContent": "Cell lines were treated with 753B and tested for cell viability. In vivo studies used PDX models."
    },
    {
      "@id": "section3",
      "@type": "fabio:Section",
      "dcterms:title": "Results",
      "fabio:hasContent": "753B was shown to effectively reduce cell viability in AML cell lines and improve chemotherapy outcomes by targeting senescent cells."
    },
    {
      "@id": "section4",
      "@type": "fabio:Section",
      "dcterms:title": "Discussion",
      "fabio:hasContent": "The findings suggest that 753B may offer a new strategy to overcome resistance to chemotherapy in AML."
    }
  ],
